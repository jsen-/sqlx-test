{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"clear": true
			},
			"label": "rust: cargo build"
		},
		{
			"type": "shell",
			"command": "cargo run",
			"problemMatcher": [
				"$rustc"
			],
			"label": "run",
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}